<view class="container">
  <swiper 
  indicator-dots="{{indicatorDots}}" 
  autoplay="{{autoplay}}" 
  interval="{{interval}}" 
  duration="{{duration}}" 
  circular="{{circular}}"
  >
    <block wx:for="{{swiperImages}}">
      <swiper-item>
        <image src="{{item}}" class="swiperImage"/>
      </swiper-item>
    </block>
  </swiper>
</view>
