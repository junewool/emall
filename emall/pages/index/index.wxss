.banner {
  height: 425rpx;
  width: 100%;
}
.bannerImage {
  height: 425rpx;
  width: 100%;
}
.navBox {
  height: 95rpx;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
}
.nav {
  height: 95rpx;
  width: 18%;
  font-size: 33rpx;
  text-align: center;
  display: inline-block;
  background: #eee;
}
.nav.active {
  background: #da4a20;
  color:#fff;
}
.navText {
  height: 95rpx;
  width: 20%;
  line-height: 95rpx;
}
.goods {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  margin-top: 15rpx;
  background-color: #f7f7f7;
  float: left;
}
.item {
  width: 50%;
  background-color: #f7f7f7;
}
.item .itemDetail {
  height: 340rpx;
  width: 360rpx;
  margin: 0 auto;
  background-color: #fff;
  margin-top: 20rpx;
}
.item:nth-child(odd) .itemDetail {
}
